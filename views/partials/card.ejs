<div class="card text-center" style="width: 19rem; min-height: 580px; display: flex; flex-direction: column;">
    <img src=<%= cardImgPath %> class="card-img-top" alt="...">
    <div class="card-body d-flex flex-column justify-content-between">
      <h5 class="card-title"><%= cardTitle %></h5>
      <p class="card-text"><%= cardDesc %></p>
      <div class="mt-auto">
        <a href=<%= link %> class="btn btn-primary" target="_blank">Go</a>
      </div>
    </div>
  </div>